{
  "expo": {
    "name": "KeepAnEyeApp",
    "slug": "KeepAnEyeApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/UpscaleLogo.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/UpscaleLogo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NsAppTransportSecurity": "NSAllowArbitraryLoads"
      },
      "icon": "./assets/UpscaleIcon.jpg",
      "bundleIdentifier": "info.plist",
      "buildNumber": "1.0.0",
      "splash": {
        "image": "./assets/UpscaleLogo.jpg",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    "android": {
      "icon": "./assets/UpscaleIcon.jpg",
      "splash": {
        "image": "./assets/UpscaleLogo.jpg",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/UpscaleIcon.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.NexerKAE.KeepAnEyeApp",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/splash-icon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/UpscaleLogo.jpg",
          "dark": {
            "image": "./assets/UpscaleLogo.jpg"
          }
        }
      ]
    ]
  }
}
