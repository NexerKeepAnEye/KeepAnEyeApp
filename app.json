{
    "expo": {
    "name": "KeepAnEyeApp",
    "slug": "KeepAnEyeApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/UpscaleLogo.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/UpscaleLogo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NsAppTransportSecurity": "NSAllowArbitraryLoads"
      }
    },
    "android": {
      "icon": "./assets/UpscaleIcon.jpg",
      "splash": {
      "image": "./assets/UpscaleLogo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
      "adaptiveIcon": {
        "foregroundImage": "./assets/UpscaleIcon.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.NexerKAE.KeepAnEyeApp"
    },
    "web": {
      "favicon": "./assets/splash-icon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
          }
        ],
        [
          "expo-splash-screen",
          {
            "image": "./assets/UpscaleLogo.jpg",
            "dark": {
              "image": "./assets/UpscaleLogo.jpg"
            }
            // "imageWidth": 200
          }
      ]
    ]
  }
}
